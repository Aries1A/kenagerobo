    "from socket import socket, AF_INET, SOCK_DGRAM\n",
    "def send_goHome(goHome):\n",
    "    ADDRESS = \"157.82.207.141\" # M5Stack address\n",
    "    PORT = 8000\n",
    "    s = socket(AF_INET, SOCK_DGRAM)\n",
    "    s.sendto(str(goHome).encode(), (ADDRESS, PORT))\n",
    "    s.close()\n",
